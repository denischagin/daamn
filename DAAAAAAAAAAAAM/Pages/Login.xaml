<Page x:Class="DAAAAAAAAAAAAM.Pages.LogIn"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DAAAAAAAAAAAAM.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
        <StackPanel>

            <Label Content="Label" HorizontalAlignment="Left" />
            <TextBox x:Name="username" HorizontalAlignment="Center" Text="TextBox" />
            <Label Content="Label"/>
            <PasswordBox x:Name="password" />
            <Button Content="Button" HorizontalAlignment="Center" Margin="0,190,0,0" VerticalAlignment="Top" Width="780" Click="Button_Click"/>
            <Button Content="Зарегистироваться" Click="Registration_Click"  />

        </StackPanel>

    </Grid>
</Page>
    
